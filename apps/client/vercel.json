{
    "rewrites": [
        {
            "source": "/(.*).js",
            "destination": "/$1.js"
        },
        {
            "source": "/(.*).css",
            "destination": "/$1.css"
        },
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
